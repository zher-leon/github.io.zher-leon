{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@api/*": ["src/api/*"],
      "@store/*": ["src/store/*"],
      "@utils/*": ["src/static/utils/*"],
      "@styles/*": ["src/static/styles/*"],
      "@icons/*": ["src/components/icons/*"],
      "@components/*": ["src/components/*"],
      "@views/*": ["src/views/*"],
      "@hooks/*": ["src/hooks/*"],
      "@router/*": ["src/router"]
    }
  }
}
